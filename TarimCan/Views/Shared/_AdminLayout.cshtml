@using TarimCan.Helper

<!DOCTYPE html>

<html lang="en" class="no-js">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>Tarım Can Yeni Nesil Çiftlik Yardımcınız</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="tarimcan, terra seracılık, terra bilişim, sürü yönetimi, çiftlik yönetimi, sera yönetimi, yazılım program" />
    <meta content="" name="Terra Bilişim Hizmetleri" />
    <!-- BEGIN GLOBAL MANDATORY STYLES

    <link href="../DashboardTheme/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
        -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/font-awesome/css/font-awesome.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/simple-line-icons/simple-line-icons.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap/css/bootstrap.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/uniform/css/uniform.default.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/fullcalendar/fullcalendar.min.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/jqvmap.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/admin/pages/css/tasks.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/select2/select2.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/plugins/datatables/plugins/bootstrap/dataTables.bootstrap.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/css/components-rounded.css")" type="text/css" id="style_components" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/global/css/plugins.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/admin/layout4/css/layout.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/admin/layout4/css/themes/light.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/admin/layout4/css/custom.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/DashboardTheme/assets/admin/pages/css/profile.css")" type="text/css" />

    <link rel="stylesheet" href="https://site-assets.fontawesome.com/releases/v6.1.2/css/all.css" type="text/css" />

</head>

<body class="page-header-fixed page-sidebar-closed-hide-logo page-sidebar-closed-hide-logo">
    <!-- BEGIN HEADER -->
    <div class="page-header navbar navbar-fixed-top">
        <!-- BEGIN HEADER INNER -->
        <div class="page-header-inner">
            <!-- BEGIN LOGO -->
            <div class="page-logo">
                <a href="/Home/Index">
                    <img src="@Url.Content("~/DashboardTheme/assets/admin/layout4/img/logo-light.png")" alt="logo" class="logo-default" style=" width: 170px;" />
                </a>
                <div class="menu-toggler sidebar-toggler">
                    <!-- DOC: Remove the above "hide" to enable the sidebar toggler button on header -->
                </div>
            </div>
            <!-- END LOGO -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
            </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            <!-- BEGIN PAGE TOP -->
            <div class="page-top">
                <div class="search-form">
                    <div class="input-group">
                        <input type="text" class="form-control input-sm" placeholder="Search..." name="query" autocomplete="off">

                        <span class="input-group-btn">
                            <a href="javascript:;" class="btn submit"><i class="icon-magnifier"></i></a>
                        </span>
                    </div>

                </div>
                <div class="top-menu">
                    <ul class="nav navbar-nav pull-right">
                        <li class="dropdown dropdown-user dropdown-dark" onclick="GoToProfile()">
                            <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                                <span class="username username-hide-on-mobile">
                                    @SessionManager.IsimSoyisim
                                </span>
                                <img class="img-circle" src="@Url.Content("~/DashboardTheme/assets/admin/layout4/img/avatar.png")" />
                            </a>
                        </li>

                        <!-- BEGIN USER LOGIN DROPDOWN -->
                        <li class="dropdown dropdown-extended quick-sidebar-toggler">
                            <span class="sr-only">Toggle Quick Sidebar</span>
                            <i class="icon-logout" onclick="LogOut()"></i>
                        </li>
                        <!-- END USER LOGIN DROPDOWN -->
                    </ul>
                </div>
                <!-- END TOP NAVIGATION MENU -->
            </div>
            <!-- END PAGE TOP -->
        </div>
        <!-- END HEADER INNER -->
    </div>
    <!-- END HEADER -->
    <div class="clearfix">
    </div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN SIDEBAR -->
        <div class="page-sidebar-wrapper">
            <!-- DOC: Set data-auto-scroll="false" to disable the sidebar from auto scrolling/focusing -->
            <!-- DOC: Change data-auto-speed="200" to adjust the sub menu slide up/down speed -->
            <div class="page-sidebar navbar-collapse collapse">

                <ul class="page-sidebar-menu" style="min-height: 600px !important;" data-keep-expanded="false" data-auto-scroll="true" data-slide-speed="200">

                    <li id="AnaSayfa">
                        <a href="/Home/Index">
                            <i class="icon-home"></i>
                            <span class="title">Ana Sayfa</span>
                        </a>
                    </li>
                    <li id="tanimlamalar">
                        <a href="javascript:;">
                            <i class="icon-docs"></i>
                            <span class="title">Tanımlamalar</span>
                            <span class="arrow" id="tanimlamalarArrow"></span>
                        </a>

                        <ul class="sub-menu" id="tanimlamalarSubMenu">

                            <li id="subMenuHayvanIrklari">
                                <a href="/Tanimlamalar/HayvanIrklari">Hayvan Irkları Tanımlama</a>
                            </li>
                            <li id="subMenuDurumBilgisi">
                                <a href="/Tanimlamalar/DurumBilgisi">Durum Bilgisi Tanımlama</a>
                            </li>
                            <li id="subMenuHayvanStatusu">
                                <a href="/Tanimlamalar/HayvanStatuleri">Hayvan Statüsü Tanımlama</a>
                            </li>
                        </ul>
                    </li>
                </ul>

                <div>
                    <img src="~/DashboardTheme/assets/admin/pages/img/beta_img.jpg" width="100" height="85" />
                </div>
            </div>
        </div>
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper" style="margin-top: -10px !important;">
            <div class="page-content">
                <div class="row">
                    @RenderBody()
                </div>
                <a href="javascript:;" class="page-quick-sidebar-toggler"><i class="icon-login"></i></a>
            </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <div class="page-footer">
        <div class="page-footer-inner">
            2022 &copy; Tarımcan Tüm Hakları Saklıdır.
        </div>
        <div class="scroll-to-top">
            <i class="icon-arrow-up"></i>
        </div>
    </div>
    <!-- END FOOTER -->

    <link href="@Url.Content("~/DashboardTheme/assets/admin/layout4/css/custom.css")" type="text/css" rel="stylesheet" />
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery-migrate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery-ui/jquery-ui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery.blockui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery.cokie.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/uniform/jquery.uniform.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/jquery.vmap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.russia.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.world.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.europe.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.germany.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.usa.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jqvmap/jqvmap/data/jquery.vmap.sampledata.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/jquery.sparkline.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/select2/select2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/plugins/bootbox/bootbox.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/global/scripts/metronic.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/DashboardTheme/assets/admin/layout4/scripts/layout.js")" type="text/javascript"></script>

    <script src="https://use.fortawesome.com/1ce05b4b.js"></script>

    <script>
        jQuery(document).ready(function () {
            Metronic.init();
            Layout.init();
        });</script>

    <script>

        function GoToProfile() {
            location.href = "/Profil/Index";
        }

        function LogOut() {
            location.href = "/Home/LogOut";
        }

        function ShowErrorMessageBox(message) {
            bootbox.dialog({
                message: message,
                title: "Hata !", buttons: {
                    danger: { label: "Tamam", className: "red" }
                }
            });
        }

        function ShowInfoFunctionBox(message) {
            bootbox.dialog({
                message: message,
                title: "Bilgi !", buttons: {
                    danger: { label: "Tamam", className: "orange" }
                }
            });
        }

    </script>
</body>
<!-- END BODY -->
</html>